{
  "name": "@ObserveRTC/webextrapp-lib",
  "version": "0.1.2",
  "description": "Extractor Development Toolkits for WebRTC Samples",
  "main": "dist/webextrapp-lib.js",
  "dependencies": {},
  "devDependencies": {
    "@types/fingerprintjs2": "^2.0.0",
    "@types/uuid": "^8.0.0",
    "@types/webrtc": "0.0.26",
    "@types/ws": "^7.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "fingerprintjs2": "^2.1.0",
    "loglevel": "^1.6.8",
    "nodemon": "^2.0.4",
    "reconnecting-websocket": "^4.4.0",
    "rxjs": "^6.5.5",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "uuid": "^8.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "ws": "^7.3.1"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "debug": "ts-node src/index.ts",
    "build-library": "npm run build && webpack --mode=production",
    "build-and-copy-library": "npm run build && webpack --mode=development && cp -r ./dist/webextrapp-lib.* ./__test__/pc1/node_modules/@observertc/webextrapp-lib/dist",
    "publish-library": "npm run build-library && npm publish",
    "build-local-test": "npm run build && webpack --mode=development && cp -r ./dist/webextrapp-lib.* ./__test__/pc1/js",
    "local-demo": "npm run build-local-test && pushd __test__/pc1 && npm install && python2 -m SimpleHTTPServer 9090 && popd",
    "local-docker-demo": "npm run build-local-test && docker-compose down && docker-compose build --no-cache && docker-compose up webextra-app",
    "proxy-server": "docker-compose down && docker-compose build --no-cache && docker-compose up proxy-server"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "url": "git@github.com:ObserveRTC/webextrapp.git"
  },
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ]
}
