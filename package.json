{
  "name": "@ObserveRTC/observer-lib",
  "version": "0.3.1",
  "description": "Extractor Development Toolkits for WebRTC Samples",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "dependencies": {
    "@types/fingerprintjs2": "^2.0.0",
    "@types/uuid": "^8.0.0",
    "@types/webrtc": "0.0.26",
    "@types/ws": "^7.2.6",
    "fingerprintjs2": "^2.1.0",
    "loglevel": "^1.6.8",
    "rxjs": "^6.5.5",
    "uuid": "^8.1.0",
    "ws": "^7.3.1",
    "reconnecting-websocket": "^4.4.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "build": "tsc",
    "build-library": "npm run build",
    "watch": "tsc -w",
    "debug": "ts-node src/index.ts",

    "build-library-dev": "npm run build-library && webpack --config webpack/dev/webpack.config.js --mode=development",
    "build-library-prod": "npm run build-library && webpack --config webpack/prod/webpack.config.js --mode=production",
    "publish-npm-library": "npm run build-library-prod && npm publish",
    "publish-cdn-library": "npm run build-library-dev && npm run build-library-prod"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "url": "git@github.com:ObserveRTC/webextrapp.git"
  }
}
